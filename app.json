{
  "expo": {
    "name": "CamLocation",
    "slug": "CamLocation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "camlocation",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.CamLocation"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CamLocation to access your camera to take photos with location data."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CamLocation to use your location to add GPS data to photos.",
          "locationWhenInUsePermission": "Allow CamLocation to use your location to add GPS data to photos."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow CamLocation to save photos to your camera roll.",
          "savePhotosPermission": "Allow CamLocation to save photos to your camera roll.",
          "isAccessMediaLocationEnabled": true,
          "android": {
            "permissions": ["READ_MEDIA_IMAGES", "WRITE_EXTERNAL_STORAGE"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1cbc3e22-2b49-4bcc-9b95-0610671abf86"
      }
    }
  }
}
